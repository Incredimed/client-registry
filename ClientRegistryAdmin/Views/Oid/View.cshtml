@model ClientRegistryAdmin.ClientRegistryAdminService.OidInfo
@{
    ViewBag.Title = "View OID";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";

}

@section AdminView
{
<div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title">Oid Details (@Model.key)</h3>
            </div>
            <div class="panel-body">
                <table border="1" style="width:80%" class="infotable">
                    <tr>
                        <th>KEY</th>
                        <td>@Model.key</td>
                    </tr>
                    <tr>
                        <th>OID</th>
                        <td>@Model.oid</td>
                    </tr>
                    <tr>
                        <th>URI</th>
                        <td>@Model.url</td>
                    </tr>
                    <tr>
                        <th>Description</th>
                        <td>@Model.Text</td>
                    </tr>
                    @if(Model.attribute != null)
                    { 
                        <tr>
                            <th colspan="2" style="text-align:center">Attributes</th>
                        </tr>
                        <tr>
                            <th style="text-align:center">Key</th>
                            <th style="text-align:center">Value</th>
                        </tr>
                        foreach(var att in Model.attribute)
                        {
                            <tr>
                                <td>@att.key</td>
                                <td>@att.value</td>
                            </tr>
                        }
                    }
                </table>
            </div>
</div>
}