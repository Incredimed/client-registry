                     <mif:position x="5.598" y="10.414"/>
                     <mif:size width="2.326" height="1.161"/>
                  </mif:graphElement>
               </mif:graphicRepresentation>
               <mif:annotations>
                  <mif:definition>
                     <mif:text>
                        <mif:p>
                           <mif:p>Indicates the item(s) being acted upon by this trigger event.</mif:p>
                        </mif:p>
                        <mif:p>
                           <mif:p>Will be mandatory in wrappers with payload messages, otherwise not present.</mif:p>
                        </mif:p>
                     </mif:text>
                  </mif:definition>
                  <mif:usageNotes>
                     <mif:text>
                        <mif:p>
                           <mif:p>If there are multiple subject items, the changes to all of them must be either accepted or rejected as a single action. I.e. One trigger event = 1 unit of work.</mif:p>
                        </mif:p>
                     </mif:text>
                  </mif:usageNotes>
                  <mif:rationale>
                     <mif:text>
                        <mif:p>
                           <mif:p>Provides information about the thing being created, modified or removed.</mif:p>
                        </mif:p>
                     </mif:text>
                  </mif:rationale>
                  <mif:constraint>
                     <mif:text>
                        <mif:p>
                           <mif:p>On ‘creation’ interactions (where the state transition is null-&gt;active), this must be true otherwise it must be false.</mif:p>
                        </mif:p>
                     </mif:text>
                     <mif:graphicRepresentation>
                        <mif:graphElement shapeId="Sheet.221" containerDiagramName="Main" isWidthAutoSize="true"
