                     <mif:text>
                        <mif:p>
                           <mif:p>Class which describes the query specifics and parameter list.</mif:p>
                        </mif:p>
                     </mif:text>
                  </mif:definition>
                  <mif:constraint>
                     <mif:text>
                        <mif:p>
                           <mif:p>InitialQuantity must only be specified if InitialQuantityCode is specified.</mif:p>
                        </mif:p>
                     </mif:text>
                     <mif:graphicRepresentation>
                        <mif:graphElement shapeId="Sheet.383" containerDiagramName="Main" isWidthAutoSize="true"
                                          isHeightAutoSize="true">
